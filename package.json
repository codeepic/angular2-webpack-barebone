{
  "name": "angular2-barebone",
  "version": "0.1.0",
  "description": "Angular 2 seed with Webpack 2, pure barebones. Built for understanding how to structure angular 2 application",
  "keywords": [
    "angular2",
    "webpack",
    "typescript",
    "barebone"
  ],
  "author": "Thomas Horvath <thomas@criticalpixel.com>",
  "homepage": "https://github.com/criticalpixel/angular2-webpack-barebone",
  "license": "MIT",
  "scripts": {
    "build"  : "webpack --config webpack.config.js",
    "express" : "node server.js",
    "server:dev" : "webpack-dev-server --config webpack.config.js --watch",
    "server:express" : "npm run build && npm run express"
  },
  "dependencies": {
    "@angular/common": "~2.1.1",
    "@angular/compiler": "~2.1.1",
    "@angular/compiler-cli": "^2.1.2",
    "@angular/core": "~2.1.1",
    "@angular/forms": "~2.1.1",
    "@angular/http": "~2.1.1",
    "@angular/platform-browser": "~2.1.1",
    "@angular/platform-browser-dynamic": "~2.1.1",
    "@angular/router": "~3.1.1",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "tslint": "^4.0.0-dev.0",
    "express": "^4.14.0",
    "concurrently": "^3.0.0",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.9",
    "webpack-dev-middleware": "^1.6.1",
    "html-webpack-plugin": "^2.24.1",
    "script-ext-html-webpack-plugin": "^1.3.2",
    "angular2-router-loader": "^0.3.3",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^2.2.1",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "raw-loader": "0.5.1",
    "source-map-loader": "^0.1.5",
    "to-string-loader": "^1.1.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/criticalpixel/angular2-barebone.git"
  },
  "bugs": {
    "url": "https://github.com/criticalpixel/angular2-barebone/issues"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
